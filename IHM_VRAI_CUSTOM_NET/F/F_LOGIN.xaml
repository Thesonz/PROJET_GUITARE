<Window x:Class="IHM_VRAI_CUSTOM_NET.F.F_LOGIN"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IHM_VRAI_CUSTOM_NET.F"
        xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen" WindowStyle="None" AllowsTransparency="True"
        Background="Transparent"
        Title="F_LOGIN" Height="650" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="320"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <fa:IconImage Icon="Close" Grid.Column="1" Panel.ZIndex="1" MouseUp="IconImage_MouseUp" Style="{StaticResource imgClose}"/>


        <Frame Grid.Column="0" x:Name="F_LoginG" NavigationUIVisibility="Hidden" />


        <Border Background="#fff" Grid.Column="1" CornerRadius="0 20 20 0" MouseDown="Border_MouseDown">
            <Frame x:Name="F_Login" NavigationUIVisibility="Hidden" />
        </Border>
    </Grid>
</Window>
